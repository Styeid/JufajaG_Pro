            android:fontFamily="@font/coda"
            android:hint="aa-bb-cc-dd-ee-ff-gg-hh-ii-jj-kk-ll"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#C9401D91"
            android:textSize="1sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/btnJA"
            app:layout_constraintEnd_toEndOf="@+id/rvJFJ"
            tools:text="xx-xx" />

    </androidx.constraintlayout.widget.ConstraintLayout>

