            android:textColor="#FFDD00"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tvYImageU"
            app:layout_constraintEnd_toEndOf="@+id/tvYImageU"
            app:layout_constraintStart_toStartOf="@+id/tvYImageU"
            tools:text="color" />

        <TextView
            android:id="@+id/tvYImageU"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/coda"
            android:hint="aa-bb-cc-dd-ee-ff-gg-hh-ii-jj-kk-ll"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#C9401D91"
            android:textSize="1sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tvXColor"
            app:layout_constraintEnd_toEndOf="@+id/tvXImageU"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tvXImageU"
            tools:text="xx-xx" />

        <TextView
            android:id="@+id/tvXCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:fontFamily="@font/coda"
            android:hint="xx"
            android:textAlignment="center"
            android:textColor="#FF6600"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/tvXColor"
            app:layout_constraintEnd_toStartOf="@+id/tvXColor"
            app:layout_constraintTop_toTopOf="@+id/tvXColor"
            tools:text="xx" />

        <TextView
            android:id="@+id/tvXColor"
