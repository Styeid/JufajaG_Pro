            android:textAlignment="center"
            android:textColor="#B3FF00"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/tvZColor"
            app:layout_constraintEnd_toStartOf="@+id/tvZColor"
            app:layout_constraintTop_toTopOf="@+id/tvZColor"
            tools:text="xx" />

        <TextView
            android:id="@+id/tvZColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/coda"
            android:hint="xx"
            android:textAlignment="center"
            android:textColor="#B3FF00"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tvZImageU"
            app:layout_constraintEnd_toEndOf="@+id/tvZImageU"
            app:layout_constraintStart_toStartOf="@+id/tvZImageU"
            tools:text="color" />

        <TextView
            android:id="@+id/tvZImageU"
            android:layout_width="wrap_content"
