<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backsplash007"
    app:columnCount="3"
    app:rowCount="4"
    tools:context=".JufajaActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvJFJ"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

        </androidx.recyclerview.widget.RecyclerView>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnRB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:visibility="invisible"
            app:backgroundTint="#19ADC7DD"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="@+id/btnJU"
            app:layout_constraintEnd_toStartOf="@+id/btnJU"
            app:srcCompat="@android:drawable/ic_dialog_dialer" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnJU"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="9dp"
            app:backgroundTint="#ADC7DD"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="@+id/btnFA"
            app:layout_constraintEnd_toStartOf="@+id/btnFA"
            app:srcCompat="@drawable/btn_ju002" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnFA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="9dp"
            android:backgroundTint="#F74B85"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="@+id/btnJA"
            app:layout_constraintEnd_toStartOf="@+id/btnJA"
            app:srcCompat="@drawable/btn_fa001" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnJA"
            android:layout_width="wrap_content"
            android:layout_height="244dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="8dp"
            android:backgroundTint="#8091FB"
            android:clickable="true"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/btn_ja001" />

        <TextView
            android:id="@+id/tvWCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:fontFamily="@font/coda"
            android:hint="xx"
            android:textAlignment="center"
            android:textColor="#00FFF7"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/tvWColor"
            app:layout_constraintEnd_toStartOf="@+id/tvWColor"
            app:layout_constraintTop_toTopOf="@+id/tvWColor"
            tools:text="xx" />

        <TextView
            android:id="@+id/tvWColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/coda"
            android:hint="xx"
            android:textAlignment="center"
            android:textColor="#00FFF7"
            android:textColorHint="#C9401D91"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tvWImageU"
            app:layout_constraintEnd_toEndOf="@+id/tvWImageU"
            app:layout_constraintStart_toStartOf="@+id/tvWImageU"
            tools:text="color" />

        <TextView
            android:id="@+id/tvWImageU"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/coda"
            android:hint="aa-bb-cc-dd-ee-ff-gg-hh-ii-jj-kk-ll"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#C9401D91"
            android:textSize="1sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tvYColor"
            app:layout_constraintEnd_toEndOf="@+id/tvYImageU"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tvYImageU"
